	help
	  exFAT disk information

config KV_STORE_KEY_BLUETOOTH_CTLR_VERSION
	bool "Enable KV key BLUETOOTH_CTLR_VERSION"
	depends on !BT_CTLR
	default y if BT
	help
	  External Bluetooth controller version

config KV_STORE_KEY_FIXED_LOCATION
	bool "Enable KV key FIXED_LOCATION"
	help
	  Fixed global location of the device

config KV_STORE_KEY_WIFI_SSID
	bool "Enable KV key WIFI_SSID"
	default y if WIFI
	help
	  WiFi network name

config KV_STORE_KEY_WIFI_PSK
	bool "Enable KV key WIFI_PSK"
	default y if WIFI
	help
	  WiFi network password

config KV_STORE_KEY_NTP_SERVER_URL
	bool "Enable KV key NTP_SERVER_URL"
	default y if SNTP
	help
	  URL of the NTP server to use for time synchronisation

config KV_STORE_KEY_EPACKET_UDP_URL
	bool "Enable KV key EPACKET_UDP_URL"
