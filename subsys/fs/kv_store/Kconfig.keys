# Autogenerated KV key options
# scripts/west_commands/cloudgen.py

config KV_STORE_KEY_REBOOTS
	bool "Enable KV key REBOOTS"
	help
	  Nunmber of times device has rebooted

config KV_STORE_KEY_BLUETOOTH_ADDR
	bool "Enable KV key BLUETOOTH_ADDR"
	default y if BT
	help
	  Bluetooth advertising address

config KV_STORE_KEY_EXFAT_DISK_INFO
	bool "Enable KV key EXFAT_DISK_INFO"
