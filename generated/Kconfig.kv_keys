config KV_STORE_KEY_DEVICE_NAME
	bool "Enable KV key DEVICE_NAME"
	default y
	help
	  Personalised name for the device

config KV_STORE_KEY_INFUSE_APPLICATION_ID
	bool "Enable KV key INFUSE_APPLICATION_ID"
	help
	  CONFIG_INFUSE_APPLICATION_ID, store will be reset if the values don't match

config KV_STORE_KEY_FIXED_LOCATION
	bool "Enable KV key FIXED_LOCATION"
